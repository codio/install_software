[Service]
User = root
Group = root
WorkingDirectory = /opt/novnc/noVNC-master/
ExecStart = /opt/novnc/noVNC-master/utils/novnc_proxy --vnc localhost:5900 --heartbeat 10 --listen 3000
Restart=always
RestartSec=5s

[Unit]
Description = NO VNC
After = x11vnc.service

[Install]
WantedBy = multi-user.target
